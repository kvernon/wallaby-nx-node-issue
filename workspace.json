{
  "version": 2,
  "projects": {
    "testing": {
      "root": "libs/testing",
      "sourceRoot": "libs/testing/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/testing/**/*.ts"] }
        }
      }
    },
    "testing-test": {
      "root": "libs/testing-test",
      "sourceRoot": "libs/testing-test/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/testing-test/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/testing-test"],
          "options": {
            "jestConfig": "libs/testing-test/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": { "defaultCollection": "@nrwl/workspace" }
}
